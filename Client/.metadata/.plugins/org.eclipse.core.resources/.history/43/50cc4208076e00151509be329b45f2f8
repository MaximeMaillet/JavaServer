package network;

import java.io.ObjectOutputStream;
import java.net.Socket;

import network.Message.TypeMessage;

public class ServerThreadListen extends Thread {

	private Socket mySocket;
	
	public ServerThreadListen(Socket _s) {
		this.mySocket = _s;
	}
	
	@Override
	public void run() {
		super.run();
		
		Message m = new Message("test", TypeMessage.MESSAGE);
		ObjectOutputStream buffOUT = new ObjectOutputStream(s.getOutputStream());
		buffOUT.writeObject(m);
	}
}
